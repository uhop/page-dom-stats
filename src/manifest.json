{
  "manifest_version": 3,
  "name": "Page DOM Stats",
  "version": "1.0.1",
  "description": "Gather statistics on DOM and CSS",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/mobile-statistics-icon-16.png",
      "24": "/images/mobile-statistics-icon-24.png",
      "32": "/images/mobile-statistics-icon-32.png",
      "48": "/images/mobile-statistics-icon-48.png",
      "64": "/images/mobile-statistics-icon-64.png",
      "72": "/images/mobile-statistics-icon-72.png",
      "96": "/images/mobile-statistics-icon-96.png",
      "128": "/images/mobile-statistics-icon-128.png"
    }
  },
  "icons": {
    "16": "/images/mobile-statistics-icon-16.png",
    "24": "/images/mobile-statistics-icon-24.png",
    "32": "/images/mobile-statistics-icon-32.png",
    "48": "/images/mobile-statistics-icon-48.png",
    "64": "/images/mobile-statistics-icon-64.png",
    "72": "/images/mobile-statistics-icon-72.png",
    "96": "/images/mobile-statistics-icon-96.png",
    "128": "/images/mobile-statistics-icon-128.png"
  },
  "author": "Eugene Lazutkin"
}
